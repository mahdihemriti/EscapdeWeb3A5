<form method="post" action="{{ path('app_hotel_delete', {'idhotel': hotel.idhotel}) }}"
      onsubmit="return confirm('Êtes-vous sûr de bien vouloir supprimer cet élément?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ hotel.idhotel) }}">
    <button class="btn">Supprimer</button>
</form>
