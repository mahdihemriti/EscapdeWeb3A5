<form method="post" action="{{ path('app_facture_delete', {'idf': facture.idf}) }}"
      onsubmit="return confirm('Êtes-vous sûr de bien vouloir supprimer cet élément?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ facture.idf) }}">
    <button class="btn"><i class="fa fa-trash"></i></button>
</form>
