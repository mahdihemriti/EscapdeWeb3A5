<form method="post" action="{{ path('app_destination_delete', {'id': dest.id}) }}"
      onsubmit="return confirm('Êtes-vous sûr de bien vouloir supprimer cet élément?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ dest.id) }}">
    <button class="btn btn-primary">Supprimer</button>
</form>
